{
  "version": 2,
  "buildCommand": "./build_files.sh",
  "builds": [
    {
      "src": "project/wsgi.py",
      
      "use": "@serverless-a/vercel-sagemaker-numpy@3.9.0", 
      
      "config": {
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    },
    {
      "src": "staticfiles/**", 
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "project.settings"
  }
}